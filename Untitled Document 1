x1 = -tooth_thickness / 2 - t + 0.5
        y1 = (tooth_external_diameter - tooth_internal_diameter) / 2
        z1 = tooth_width / 2 + w + 0.045

        x2 = tooth_thickness / 2 + t + 0.5
        y2 = y1
        z2 = z1

        x3 = x2
        y3 = y2
        z3 = z2-r

        x4 = x1
        y4 = y3
        z4 = z3 + 0.09
